{
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/",
      "dest": "/web/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ]
}