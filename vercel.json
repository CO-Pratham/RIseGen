{
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(css|js|html|png|jpg|jpeg|svg|gif|ico))",
      "dest": "/web/$1"
    },
    {
      "src": "/",
      "dest": "/web/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ]
}